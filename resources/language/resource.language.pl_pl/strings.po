msgctxt "#30001"
msgid "Connection"
msgstr "Połączenie"

msgctxt "#30002"
msgid "Home Assistant Server URL (with protocol)"
msgstr "URL Serwera Home Assistant (z protokołem)"

msgctxt "#30003"
